package {	import flash.display.MovieClip;	import flash.geom.*;	public class Square extends MovieClip	{		[Inspectable]// bring parameter up to component inspector		public var squareColor:uint;		private var squareColorApplied:ColorTransform;// applied glow color		public function Square()		{			// constructor code			squareColorApplied = new ColorTransform();			squareColorApplied.color = squareColor;					}	}}